{
  "name": "setup-jena",
  "version": "1.0.0",
  "description": "GitHub Action to setup Apache Jena",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/setup-jena.ts",
    "format": "prettier --write .",
    "format-check": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foooomio/setup-jena.git"
  },
  "author": "foooomio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foooomio/setup-jena/issues"
  },
  "homepage": "https://github.com/foooomio/setup-jena#readme",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/tool-cache": "^1.7.1",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@vercel/ncc": "^0.30.0",
    "prettier": "^2.3.2",
    "simple-git-hooks": "^2.6.1",
    "typescript": "^4.4.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run format && npm run build"
  }
}
